{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": ["dom", "es6"],
    "sourceMap": true,
    "strict": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  },
  "exclude": ["/node_modules/(?!@2gis/gl-matrix|2gl)/"],
  "ignore": ["/node_modules/(?!@2gis/gl-matrix|2gl)/"]
}
